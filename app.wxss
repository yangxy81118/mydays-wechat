.red{
  color:rgb(253,112,112)!important;
}

.orange{
  color:#ff9940!important;
}


.l-grey{
  color:#CCC;
}


.submit-btn{
  font-size:30rpx;
  display: block;
  line-height: 100rpx;
  height:100rpx;
  color:#FFF;
  background: #ff9940;
  width:100%;
  box-shadow:0 2rpx 6rpx rgba(255,153,64,0.40);
  text-align: center;
  border-radius: 10rpx;
}

button.submit-btn:after{
  border: none;
}

.show-block{
  display: block!important;
}

.show-hidden{
  display: none!important;
}


/*model popUp dialog*/
view.popUpBk{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  align-items: center;
  justify-content: center;
}

view.popUpBk view.popUpBox {
  width: 56%;
  height: auto;
  margin: 0 auto;
  background: #fff;
  border-radius: 20rpx;
  position: relative;
}

/* component */
text.dot{
  display: block;
  background: #FF001F;
  width:16rpx;
  height:16rpx;
  border-radius: 50%;
  position: absolute;
}

/*reset*/
input,button,select,textarea{outline:none}
textarea{resize:none} 