}

view.detailBox text.remain {
  color: #444;
  font-size: 38rpx;
  margin: 10rpx 0 0;
}

view.detailBox text.name {
  font-size: 38rpx;
  margin: 24rpx 0 5rpx;
  font-weight: 700;
}

view.detailBox text.birthday {
  font-size: 34rpx;
  margin: 0 0 15rpx 0;
}

view.detailBox view.greeting {
  padding:30rpx 0;
}

view.detailBox view.greeting text {
  font-size: 24rpx;
  color: #bbb;
}

view.detailBox .toolBar {
  width: 100%;
}

view.detailBox navigator {
  display: inline-block;
  font-size: 48rpx;
  color: #aaa;
  width: 50%;
  line-height: 60rpx;
  text-align: center;
}

view.detailBox .highLight{
  position: absolute;
  background: #FFF;
  height:36rpx;
  left:0;
  top:-10rpx;
  width:100%;
  z-index:9;
  border-top-right-radius: 20rpx;
  border-top-left-radius: 20rpx;
}

view.detailBox .hl{
  background: #ff9940;
}

text.icon-fenxiang {
  font-size: 54rpx !important;
}

