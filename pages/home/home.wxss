view.share-tip{
  position: absolute;
  bottom:17%;
  right:7%;
  width:40%;
  height:70rpx;
  background: #FFE8D3;
  border-radius: 28rpx;
  text-align: center;
  color:#FF9940;
  font-size:26rpx;
  line-height: 70rpx;
  box-shadow: 0 4rpx 6rpx #E8DECA;
  display: none;
}

view.share-tip view.arrow{
  position: absolute;
  bottom:-28rpx;
  right:20%;
  height:0px;
  width:0px;
  border-right:18rpx solid transparent;
  border-top:37rpx solid #FFE8D3;
  border-left:9px solid transparent;
}

