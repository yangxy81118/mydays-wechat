view.main {
  background: #f9f9f9;
  height: 100%;
  width: 100%;
}

view.tab-nav {
  background: #fff;
  height: 9.5%;
  position: relative;
  border-bottom: 1rpx solid #f3f3f3;
  border-width: 1px 0;
}

view.tab-nav view {
  height: 100%;
  background: none;
  font-size: 30rpx;
  color: #c7c7c7;
  display: inline-flex;
  align-items: center;
  justify-content: center;
  position: relative;
}

view.tab-nav view.nav {
  height:60rpx;
  width:15%;
}

view.tab-nav view.selected {
  color: #FFF;
  background: #ff9940; 
  border-radius: 30rpx;
  height:60rpx;
  width:15%;
}

view.tab-nav view.slide-btn{
  width: 13%;
}

view.tab-nav view.layout-btn {
  color:#FF9940;
}

view.tab-nav view.user-btn {
  position: absolute;
  top:0;
  left:0;
  width:150rpx;
  height:100%;
}

view.tab-nav view.user-btn text.dot{
  top:20rpx;
  right:36rpx;
}

view.tab-nav view.user-btn image{
  height:70rpx;
  width:70rpx;
  border-radius: 50%;
  border:2rpx solid #F5F5F5;
}


view.tab-nav view.layout-btn:active{
  background: #F9F9F9;
}

view.tab-nav view.layout-btn text.icon {
  font-size: 38rpx;
}

#normal-list{
  margin:0 0 0 35%;
}

#star-list{
  margin:0  21% 0 0;
}

view.nav text.icon {
  margin: 0 10rpx 0 0;
  /* font-size:28rpx; */
}

view.nav text.word {
  display: block;
}

view.empty-list {
  margin: 2% 0 0;
  width: 100%;
  height: 40%;
}

view.empty-list image {
  width: 300rpx;
  height: 300rpx;
  display: block;
  margin: 24% auto 0;
}

view.empty-list .sug {
  margin: 2% auto 0;
  font-size: 28rpx;
  color: #aaa;
  text-align: center;
}

view.empty-list text {
  display: block;
}

scroll-view.list {
  margin: 2% 0 0;
  width: 100%;
  height: 90%;
  /* background: #000; */
}

scroll-view.list view.bottom-box{
  width:100%;
  height:170rpx;
}

.day-item-h {
  overflow-x: hidden;
  width: 95%;
  height: 170rpx;
  text-align: center;
  margin: 0 5% 0 0;
}

view.item-scroll-wrap {
  width: 128%;
  position: relative;
  margin: 10rpx 0;
  padding: 0 5%;
}

view.item-scroll-wrap view {
  float: left;
  height: 146rpx;
}

view.item-scroll-wrap .day-box {
  width: 74.2%;
  background: #fff;
  position: relative;
  border-radius: 10rpx;
  box-shadow: 0 4px 4px 2px rgba(216, 216, 216, 0.2);
  overflow: hidden;
}

.day-box image.star {
  position: absolute;
  top: 10%;
  right: 2%;
  width: 26rpx;
  height: 26rpx;
}

.day-box .close-tip {
  position: absolute;
  z-index: 1;
  width: 20rpx;
  height: 100%;
  left: 0;
  top: 0;
  background: #ff9940;
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
}

.day-box text {
  display: block;
  position: absolute;
}

.day-box .day-title {
  font-size: 30rpx;
  font-weight: 700;
  top: 25%;
  left: 8%;
  color: #444;
}

.day-box .day-remain {
  font-size: 22rpx;
  bottom: 20%;
  left: 8%;
  color: #bbb;
}

.day-box .close-remain {
  color: #444;
}

.day-box .date {
  top: 40%;
  right: 6%;
  color: #8b8b8b;
  font-size: 24rpx;
}

.day-box .close-date {
  color: #ff9940;
}

.day-box text.icon-star{
  position: absolute;
  top:20rpx;
  right:20rpx;
  color:#EEE;
  font-size:22rpx;
}

.day-item-h view.item-scroll-wrap .opt-box {
  width: 25.8%;
  position: relative;
  top: 26rpx;
}

.day-item-h .opt-box text {
  display: inline-block;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  width: 50%;
  font-size: 34rpx;
}

.day-item-h .opt-box text.edit {
  background: #ccc;
}

.day-item-h .opt-box text.del {
  background: #fe7070;
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}


button.add {
  position: fixed;
  z-index: 2;
  width:62%;
  height:10%;
  bottom:4%;
  left:8%;
  background: #ff9940;
  color: #fff;
  text-align: center;
  font-size: 40rpx;
  box-shadow: 0 2rpx 6px #ff9940;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}

button:after{
  border:none;
}

button.add:active{
  background: #FA8836; 
  bottom:3.6%;
}

button.byShare{
  position: fixed;
  display: block;
  bottom:4%;
  right:8%;
  z-index: 2;
  background: #ff9940;
  width:18%;
  height:10%;
  box-shadow: 0 2rpx 6px #ff9940;
  border-radius: 10rpx;
}

button.byShare image{
  position: absolute;
  display: block;
  z-index: 3;
}

button.byShare image.wx{
  width:56rpx;
  height:56rpx;
  top:26%;
  left:32%;
}

button.byShare image.plus{
  width:34rpx;
  height:34rpx;
  bottom:24%;
  right:22%;
}


