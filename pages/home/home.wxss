/* wxAuth */
view.authBox{
  padding:10% 8%;
}


view.authBox view.tip{
  font-size: 28rpx;
  color:#444;
  text-align: center;
  padding:0 0 30rpx; 
}

view.authBox button{
  font-size: 32rpx;
  color:#FFF;
  border-radius: 8rpx;
  margin:32rpx 0 0 0 ;
  position: relative;
  box-shadow:0 0 4px rgba(216,216,216,0.25);
}

view.authBox button:after{
  border:none;
}

view.authBox button.share{
  background: #FF9940; 
}

view.authBox button.disabled{
  background: #CCC!important; 
  color:#FFF;
}

view.authBox button.auth{
  background: #41B035;
}

view.authBox button text.iconfont{
  color:#FFF;
  position: absolute;
  top:14rpx;
  left:20%;
  font-size: 54rpx;
  line-height: 54rpx;
}


/* 侧边栏 */
view.slide-bar{
  position: fixed;
  top:0;
  background: #FFF;
  height:100%;
  width:70%;
  z-index: 11;
  transition: all 0.6s ease;
  -webkit-transition: all 0.6s ease;
  border:none;
}

view.slide-bar view.profile{
  margin:0 0 60rpx 0;
}

view.profile image{
  width:130rpx;
  height:130rpx;
  border-radius: 50%;
  margin:20% auto 30rpx;
  display: block;
  border:3rpx solid #F5F5F5;
}

view.profile text{
  font-size:34rpx;
  display: block;
  text-align: center;
  font-weight: 700;
}

view.profile button{
  font-size:34rpx;
  background: #FFF;
  font-weight: 700;
}

view.slide-bar view.statistics{
  width:70%;
  padding:0 15%;
  margin:0 0 100rpx 0;
}

view.statistics > view{
  display: inline-block;
  width:50%;
  text-align: center;
  font-size: 28rpx;
}

view.statistics > view label{
  color:#999;
}

view.statistics > view text{
  margin:0 0 0 20rpx;
  font-size: 36rpx;
  font-weight: 700;
  position: relative;
  top:2rpx;
}

view.slide-bar view.menu{
  
}

view.menu .nav{
  width:100%;
  text-align: center;
  font-size:32rpx;
  color:#AAA;
  line-height: 100rpx;
  position: relative;
  background: #FFF;
}

view.menu .nav:active{
  background: #F9F9F9;
  color:#AAA;
}

view.menu .nav label{
  position: absolute;
  left:20%;
  /* top:1.5%; */
  font-size:42rpx;
  color:#CCC;
}

view.menu .nav text{
  
}

view.menu text.dot{
  top:20%;
  left:27%;
}

view.slide-shadow{
  position: fixed;
  background:rgba(0,0,0,.4);
  top:0;
  right:0;
  width:100%;
  height:100%;
  transition: all 0.6s ease;
  -webkit-transition: all 0.6s ease;
  z-index: 10;
}


